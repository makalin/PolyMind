{
  "agents": {
    "gpt": {
      "provider": "openai",
      "model": "gpt-4",
      "api_key": "sk-OPENAI_API_KEY_HERE"
    },
    "claude": {
      "provider": "anthropic",
      "model": "claude-3-opus-20240229",
      "api_key": "sk-CLAUDE_API_KEY_HERE"
    },
    "mistral": {
      "provider": "mistral",
      "model": "mistral-large",
      "api_key": "sk-MISTRAL_API_KEY_HERE"
    },
    "gemini": {
      "provider": "google",
      "model": "gemini-pro",
      "api_key": "sk-GEMINI_API_KEY_HERE"
    },
    "llama": {
      "provider": "local",
      "url": "http://localhost:11434/api/generate",
      "model": "llama3"
    },
    "custom": {
      "provider": "local",
      "url": "http://127.0.0.1:9000/chat",
      "model": "my-custom-agent"
    }
  },
  "defaults": {
    "format": "markdown",
    "output_dir": "./outputs",
    "history_file": "./history.json",
    "view_mode": "rich"
  },
  "aliases": {
    "explain": "gpt:explain like I'm five",
    "bugfix": "gpt:fix the following code",
    "summarize": "claude:summarize this text"
  }
}
